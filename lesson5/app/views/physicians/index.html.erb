<div class = "row justify-content-center m-3">
  <div class = "col-8">
    <div class = "card">
      <div class = "card-body">
        <h3>Physicians</h3>
        <table class = "table">
          <thead>
            <tr>
              <th>ID</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Phone number</th>
              <th>Level</th>
            </tr>
          </thead>
          <tbody>
          <% @physician.each do |physician| %>
            <tr>
              <td><%= link_to physician.id, physician %></td>
              <td><%= physician.first_name %></td>
              <td><%= physician.last_name %></td>
              <td><%= physician.phone_number %></td>
              <td><%= physician.level %></td>
            </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
