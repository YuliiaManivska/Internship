<div class ="row justify-content-center">
  <div class = "col-8">
    <div class = "card">
      <div class = "card-header">
        <div class = "row m-4">
          <h2>Create new physician</h2>
          <%= form_for :physician, url: physicians_path do |f| %>
            <p>
              First Name
              <%= f.text_field :first_name %>
            </p>
            <p>
              Last Name
              <%= f.text_field :last_name %>
            </p>
            <p>
              Phone number
              <%= f.text_field :phone_number %>
            </p>
            <p>
              Level
              <%= f.text_field :level %>
            </p>
            <p>
              <%= f.submit %>
            </p>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>


